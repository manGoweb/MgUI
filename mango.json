{
  "src_folder": "src",
  "dist_folder": "dist",
  "styles": [
    "src/styles/index.styl",
    "src/styles/oldie.styl",
    "src/styles/print.styl"
  ],
  "scripts": [
    "src/scripts/index.es6"
  ],
  "images": [
    "src/images/**/*.{jpg,png}"
  ],
  "sprites": [
    {
      "path": "src/images/sources/*.svg",
      "name": false
    }
  ],
  "static": [
    "src/fonts/**",
    "src/images/*.svg",
    "node_modules/lt-ie-9/lt-ie-9.min.js"
  ],
  "templates": [
    "src/templates/*.jade"
  ],
  "dependencies": [
    "lt-ie-9",
    "stylus-normalize",
    "jquery"
  ],
  "data": {
    "global": "src/data/global.json"
  },
  "hooks": {
    "init": "mango install"
  },
  "version": ">=0.17"
}
